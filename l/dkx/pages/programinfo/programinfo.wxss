  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  /*background-color:#D8D8D8;*/
  overflow: hidden;
  background-color: #f7f9f8;
  border-top: 1rpx solid #d8d8d8;
  margin-bottom: 100rpx;
}

.logo {
  width: 750rpx;
  display: flex;
}

.logoimage {
  width: 750rpx;
  /*height: 405rpx;*/
  height: 360rpx;
  margin-left: 0rpx;
  margin-top: 0rpx;
}

.classname {
  height: 80rpx;
  line-height: 80rpx;
  width: 750rpx;
  margin-top: 10rpx;
  display: flex;
}

.classname text {
  text-align: center;
  width: 750rpx;
  height: 80rpx;
  font-family: "MicrosoftYaHei";
  line-height: 80rpx;
  font-size: 42rpx;
  font-weight: bold;
  /*color: #999;*/
  letter-spacing: 4rpx;
}

.introduce {
  width: 750rpx;
  margin-top: 20rpx;
  display: flex;
}

.introduce text {
  width: 700rpx;
  margin-left: 30rpx;
  font-size: 36rpx;
  line-height: 50rpx;
  letter-spacing: 2rpx;
  /*color: #999;*/
  /*font-weight: 300;*/
}

button {
  position: fixed;
  bottom: 0rpx;
  color: white;
  font-size: 40rpx;
  border-radius: 0rpx;
}

.left {
  background-color: #4bc1d2;
  width: 50%;
  left: 0rpx;
}

.right {
  background-color: #81d8d0;
  width: 50%;
  right: 0rpx;
}

.one {
  width: 100%;
  left: 0rpx;
  background-color: #4bc1d2;
}

.images {
  margin-top: 20rpx;
  width: 750rpx;
}

.weui-uploader__hd {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding-bottom: 10px;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.weui-uploader__title {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  color: #b2b2b2;
  font-size: 36rpx;
}

.weui-uploader__info {
  color: #b2b2b2;
}

.weui-uploader__bd {
  margin-bottom: -4px;
  margin-right: -9px;
  overflow: hidden;
}

.weui-uploader__file {
  float: left;
  margin-right: 9px;
  margin-bottom: 9px;
  position: relative;
}

.weui-uploader__img {
  display: block;
  width: 210rpx;
  height: 210rpx;
}

.weui-uploader__file_status {
  position: relative;
}

.weui-uploader__file_status:before {
  content: " ";
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.5);
}

.weui-uploader__input-box:before, .weui-uploader__input-box:after {
  content: " ";
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}

.weui-uploader__input-box:before {
  width: 2px;
  height: 39.5px;
}

.weui-uploader__input-box:after {
  width: 39.5px;
  height: 2px;
}

.weui-uploader__input-box:active {
  border-color: #999;
}

.weui-uploader__input-box:active:before, .weui-uploader__input-box:active:after {
  background-color: #999;
}

.weui-cells {
  position: relative;
  margin-top: 10rpx;
  padding-top: 10rpx;
  line-height: 1.41176471;
  font-size: 17rpx;
  margin-left: 10rpx;
}

.weui-cells:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1rpx;
  /*border-top: 1rpx solid #d9d9d9;*/
  color: #d9d9d9;
}

.weui-cells:after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1rpx;
  /*border-bottom: 1rpx solid #d9d9d9;*/
  color: #d9d9d9;
}

.weui-cell {
  padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.weui-cell:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  /*border-top: 1rpx solid #d9d9d9;*/
  color: #d9d9d9;
}

.weui-cell:first-child:before {
  display: none;
}

.weui-cell_active {
  background-color: #ececec;
}

.weui-cell_primary {
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
  align-items: flex-start;
}

.weui-cell__bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}

.weui-cell__ft {
  text-align: right;
  color: #999;
}

.weui-uploader__im_view {
  height: 50rpx;
  width: 100%;
  background-color: black;
  background: rgba(0, 0, 0, 0.5);
  position: absolute;
  bottom: 0;
  line-height: 50rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.deleimg {
  width: 40rpx;
  height: 40rpx;
}

.tips {
  position: absolute;
  top: 80rpx;
  left: 20rpx;
  width: 300rpx;
  font-size: 28rpx;
