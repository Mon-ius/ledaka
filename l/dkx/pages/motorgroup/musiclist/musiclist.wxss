/* pages/motorgroup/musiclist/musiclist.wxss */
@import "../../../utils/searchbar/searchbar.wxss";
.music{
  width: 750rpx;
  height: 100%;
  margin-top:20rpx;
  display: flex;
  flex-direction: column;
  overflow: hidden;
  background-color: #EFEFF4;
}
.music_list{
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  background-color: white;
  overflow: hidden;
  font-size: 32rpx;
 border-bottom:1rpx solid #EFEFF4;
}
.music_list text:nth-child(1){
  margin-left:30rpx;
  
  color:#353535;
}
.music_list text:nth-child(2){
  flex:1;
  color:#888888;
  margin-left:15rpx;
}
.music_list image{
  height: 60rpx;
  width: 60rpx;
  margin-top:30rpx;
  margin-right:30rpx;
}